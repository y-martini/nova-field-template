{
    "name": "yuriy-martini/nova-package-name",
    "require": {
        "php": "^7.1 || ^7.2",
        "laravel/nova": "<4.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.6",
        "phpstan/phpstan": ">=1.0",
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YuriyMartini\\NovaPackageName\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YuriyMartini\\NovaPackageName\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuriyMartini\\NovaPackageName\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
